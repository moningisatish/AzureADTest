List<EdtcsrpolicyDTO> edtcsrpolicyList = new ArrayList<>();
		EdtcsrpolicyDTO edtcsrpolicyDTO;
		for(Edtcsrpolicy edtcsrpolicy : records) {
			edtcsrpolicy.setTimezone(request.getTimeZone());
			edtcsrpolicyDTO = mapper.toDto(edtcsrpolicy);
			if(edtcsrpolicy.getEdtcsrclientcompany() != null) {
				edtcsrpolicyDTO.setEdicompno(edtcsrpolicy.getEdtcsrclientcompany().getClientcompanyidhsbassigned());
			}
			List<Edtcsrins> edtcsrInList = edtcsrpolicy.getEdtcsrins();
			if (edtcsrInList != null && !edtcsrInList.isEmpty()) {
				List<EdtcsrinDTO> insuranceList = new ArrayList<>();
				for(Edtcsrins edtcsrIn : edtcsrInList) {
					insuranceList.add(edtcsrInsMapper.toDTO(edtcsrIn));
				}
				edtcsrpolicyDTO.setEdtcsrins(insuranceList);
			}

			List<Edtcsrloc> edtcsrlocList = edtcsrpolicy.getEdtcsrlocs();
			if (edtcsrlocList != null && !edtcsrlocList.isEmpty()) {
				List<EdtcsrlocDTO> edtcsrlocDTOList = new ArrayList<>();
				for(Edtcsrloc edtcsrloc : edtcsrlocList) {
					edtcsrlocDTOList.add(edtcsrlocMapper.toDTO(edtcsrloc));
				}
				edtcsrpolicyDTO.setEdtcsrlocs(edtcsrlocDTOList);
			}

			List<Edtcsrpvline> edtcsrpvlineList = edtcsrpolicy.getEdtcsrpvlines();
			if (edtcsrpvlineList != null && !edtcsrpvlineList.isEmpty()) {
				List<EdtcsrpvlineDTO> edtcsrpvlineDTOList = new ArrayList<>();
				for(Edtcsrpvline edtcsrpvline : edtcsrpvlineList) {
					edtcsrpvlineDTOList.add(edtcsrpvlineMapper.toDTO(edtcsrpvline));
				}

				edtcsrpolicyDTO.setEdtcsrpvlines(edtcsrpvlineDTOList);
			}
			edtcsrpolicyList.add(edtcsrpolicyDTO);
		}

	
